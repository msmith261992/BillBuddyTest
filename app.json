{
  "expo": {
    "name": "BillBuddyTest",
    "slug": "BillBuddyTest",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "packagerOpts": {
			"sourceExts": ["js", "jsx", "scss", "sass", "ttf"],
			"transformer": "node_modules/react-native-sass-transformer/index.js"
		},
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
